{
  "name": "site-rapido",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://siterapido.github.io/siterapido",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:responsive-images && npm run optimize:images && vite build",
    "build:fast": "vite build",
    "optimize:images": "node optimize-images.js",
    "generate:responsive-images": "node scripts/generate-responsive-images.js",
    "check:images": "node check-slide-images.js",
    "build-and-check": "npm run build && npm run check:images",
    "analyze": "node analyze-bundle.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "performance:audit": "npm run build && lighthouse dist/index.html --output=json --output-path=./lighthouse-report.json"
  },
  "dependencies": {
    "@number-flow/react": "^0.5.10",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@studio-freight/lenis": "^1.0.42",
    "@supabase/supabase-js": "^2.51.0",
    "@types/gsap": "^1.20.2",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.5",
    "gsap": "^3.13.0",
    "lenis": "^1.3.4",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.23.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.3",
    "tailwindcss": "3.4",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.3",
    "vite-plugin-compression2": "^2.2.0"
  }
}
